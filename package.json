{
  "name": "@flokyorch/starwars-names",
  "version": "0.0.0-development",
  "description": "Get random Star Wars names",
  "main": "src/index.js",
  "scripts": {
    "commit": "git-cz",
    "check-coverage": "nyc check-coverage --statements 100 --branches 100 --functions 100 --lines 100",
    "test": "mocha src/index.test.js -w",
    "test:single": "nyc --reporter=html --reporter=text mocha src/index.test.js",
    "semantic-release": "semantic-release",
    "travis-deploy-once": "travis-deploy-once"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kashano/starwars-names.git"
  },
  "keywords": [
    "random",
    "star",
    "wars"
  ],
  "author": "Jorge Vargas <jorge.vargas+npm@ppp.ninja> (http://www.ppp.ninja/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kashano/starwars-names/issues"
  },
  "homepage": "https://github.com/kashano/starwars-names#readme",
  "dependencies": {
    "unique-random-array": "1.0.1"
  },
  "devDependencies": {
    "chai": "4.2.0",
    "commitizen": "3.0.4",
    "cz-conventional-changelog": "2.1.0",
    "husky": "1.2.0",
    "mocha": "5.2.0",
    "nyc": "13.1.0",
    "semantic-release": "15.12.1",
    "semantic-release-cli": "4.0.12",
    "travis-deploy-once": "5.0.9"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  }
}
